A in B is A ∈ B;
A not in B is A ∉ B;

A != B is ${{ A != B }}$;
A ≠ B is ${{ A != B }}$;
A = B is ${{ A == B }}$;
= A B is ${{ A == B }}$;
//A = undefined is ${{ A === undefined }}$;

A equals B is A = B;
A not equals B is A ≠ B;

/* Force a list to be evaluated instead of returned */
eval A is A when (A ∉ lists);

/* Ignore the LHS */
discard A is [];
A ";" B is B;

